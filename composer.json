{
  "name": "contestio/magento",
  "description": "Contestio module for Magento 2",
  "type": "magento2-module",
  "version": "1.0.0",
  "require": {
    "php": "^7.4 || ^8.0",
    "magento/framework": "^102.0.0"
  },
  "autoload": {
    "files": [
      "registration.php"
    ],
    "psr-4": {
      "Contestio\\Connect\\": ""
    }
  },
  "extra": {
    "map": [
      [
        "*",
        "Contestio/Connect"
      ]
    ]
  }
}
